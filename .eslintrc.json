{
  "parser": "@typescript-eslint/parser",
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "jest": true,
    "jasmine": true
  },
 "rules": {
    "max-len": [
      "error",
      {
        "code": 200,
        "tabWidth": 2,
        "ignoreStrings": true
      }
    ],
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    ],
    "import/prefer-default-export": "off"
  },
  "overrides": [
    {
      "files": [
        "**/*.ts",
        "**/*.tsx"
      ],
      "rules": {
        "no-unused-vars": [
          "off"
        ]
      }
    }
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}